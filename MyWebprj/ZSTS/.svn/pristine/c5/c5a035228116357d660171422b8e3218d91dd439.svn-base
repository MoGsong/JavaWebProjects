package com.gxnu.entity;

public class Operator {
	private int id;
	private String username;
	private String password;
	private String tel;
	private String pic;
	private Role role;
	private String pre;

	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getTel() {
		return tel;
	}
	public void setTel(String tel) {
		this.tel = tel;
	}
	public String getPic() {
		return pic;
	}
	public void setPic(String pic) {
		this.pic = pic;
	}
	public String getPre() {
		return pre;
	}
	public void setPre(String pre) {
		this.pre = pre;
	}
	public Role getRole() {
		return role;
	}
	public void setRole(Role role) {
		this.role = role;
	}
	
	//把所有拼接成字符串，便于显示
	@Override
	public String toString() {
		return "Operator [id=" + id + ", username=" + username + ", password=" + password + ", tel=" + tel + ", pic="
				+ pic + ", pre=" + pre + ", role=" + role + "]";
	}
	
	//构造方法，为属性赋值
	public Operator(int id, String username, String password, String tel, String pic, Role role, String pre) {
		super();
		this.id = id;
		this.username = username;
		this.password = password;
		this.tel = tel;
		this.pic = pic;
		this.role = role;
		this.pre = pre;
	}
	public Operator(String username, String password, String tel, String pic, Role role,String pre) {
		super();
		this.username = username;
		this.password = password;
		this.tel = tel;
		this.pic = pic;
		this.role = role;
		this.pre = pre;
	}
	public Operator() {
		super();
	}
	
	
}

