		<!-- Shop Register -->
		<section class="register section">
			<div class="container">
				<div class="inner">
					<div class="row"> 
						<div class="col-lg-6">
							<div class="register-left">
							</div>
						</div>
						<div class="col-lg-6">
							<div class="register-form">
								<h2>患者注册</h2>
								<p>已有帐号 ? <a href="Login.jsp">在此登录</a></p>

								<!-- Form -->
								<form class="form" method="post" action="<%=Data.URL%>/register" enctype="multipart/form-data">
									<div class="row">
										<div class="col-lg-12">
											<div class="form-group">
												姓名：<input type="text" name="name" placeholder="name" required="">
											</div>
										</div>
										<div class="col-lg-12">
												性别：&nbsp;&nbsp;<input type="radio" name="sex" value="男" style="zoom:1;"/>男&nbsp;&nbsp;
											<input type="radio" name="sex" value="女" style="zoom:1;"/>女</br>
										</div></br></br>
										<div class="col-lg-12">
											<div class="form-group">
												年龄：<input type="text" name="age" placeholder="age" required="">
											</div>
										</div>
										<div class="col-lg-12">
											<div class="form-group">
												账号：<input type="text" name="username" placeholder="username" required="">
											</div>
										</div>
										<div class="col-lg-12">
											<div class="form-group">
												密码：<input type="password" name="password" placeholder="password" required="">
											</div>
										</div>
										<div class="col-lg-12">
											<div class="form-group">
												头像：<input type="file" name="pic" value="上传" >
												<img scr="" alt="">
											</div>
										</div>
										<div class="col-lg-12">
											<div class="form-group">
												身份证号码<input type="text" name="idcard" placeholder="idcard" required="">
											</div>
										</div>
										<div class="col-lg-12">
											<div class="form-group">
												电话<input type="text" name="tel" placeholder="tel" required="">
											</div>
										</div>
										<div class="col-lg-12">
											<div class="form-group">
												邮箱
												<input type="email" name="email" placeholder="email " required=""> 
												 <!--<input type="email" name="email" class="form-control" placeholder="email" required="true">-->
												
											</div>
										</div>
										
										<div class="col-12">
											<div class="form-group login-btn">
												<button class="btn" type="submit">注册</button>
											</div>											
										</div>
									</div>
								</form>
								<!--/ End Form -->
							</div>
						</div>
					</div>
				</div>
			</div>
